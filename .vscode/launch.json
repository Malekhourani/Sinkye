    "version": "0.2.0",
    "configurations": [
        {
            
            "log": true,
            "stopOnEntry": true,
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9090
        },
        {
            "name": "Launch currently open script",
            "runtimeExecutable": "c:\\xampp\\php\\php.exe",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9090
        }
    ]
}